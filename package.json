{
  "name": "gulp-stylelint-checkstyle-reporter",
  "version": "0.0.3",
  "description": "gulp-stylelint reporter that writes stylelint results to xml file in checkstyle plugin format",
  "main": "dist/index.js",
  "scripts": {
    "transpile": "babel src --out-dir dist",
    "prepublish": "npm run lint && npm run transpile",
    "lint": "eslint src/**/*.js",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/olegskl/gulp-stylelint-checkstyle-reporter.git"
  },
  "keywords": [
    "stylelint",
    "reporter",
    "checkstyle",
    "postcss",
    "css"
  ],
  "author": "Oleg Sklyanchuk <sklyanchuk@gmail.com> (http://olegskl.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/olegskl/gulp-stylelint-checkstyle-reporter/issues"
  },
  "homepage": "https://github.com/olegskl/gulp-stylelint-checkstyle-reporter",
  "dependencies": {
    "checkstyle-formatter": "^1.0.0",
    "fs-promise": "^0.5.0",
    "gulp-util": "^3.0.7",
    "mkdirp-promise": "^1.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.6.4",
    "babel-core": "^6.6.4",
    "babel-preset-es2015": "^6.6.0",
    "eslint": "^2.2.0",
    "eslint-config-meetic": "^2.0.0"
  }
}
